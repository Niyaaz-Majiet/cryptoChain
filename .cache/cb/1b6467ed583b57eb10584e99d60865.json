`, {\r\n            method: 'POST',\r\n            headers: {\r\n                'Content-Type': 'application/json'\r\n            },\r\n            body: JSON.stringify({ recipient, amount })\r\n        }).then((res) => res.json())\r\n            .then((json) => {\r\n                alert(json.message || json.type);\r\n                history.push('/transaction-pool');\r\n            });\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div className=\"ConductTransaction\">\r\n                <Link to='/'>Home</Link>\r\n                <h3>Conduct a Transaction</h3>\r\n                <FormGroup>\r\n                    <FormControl\r\n                        input='text'\r\n                        placeholder='recipient'\r\n                        value={this.state.recipient}\r\n                        name='recipient'\r\n                        onChange={(e) => this.updateRecipient(e)}\r\n                    />\r\n                </FormGroup>\r\n                <FormGroup>\r\n                    <FormControl\r\n                        input='number'\r\n                        placeholder='amount'\r\n                        name='amount'\r\n                        value={this.state.amount}\r\n                        onChange={(e) => this.updateAmount(e)}\r\n                    />\r\n                </FormGroup>\r\n                <div>\r\n                    <Button\r\n                        bsStyle=\"danger\"\r\n                        onClick={this.conductTransaction}\r\n                    >\r\n                        Submit\r\n                    </Button>\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"0bd548e27585851b53ad577881e6e0d0","cacheData":{"env":{}}}